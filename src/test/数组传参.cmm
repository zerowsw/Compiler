/*
 *函数数组传参
 */ 

void function(real array[],int add){
  
    for(int i=0;i<6;i=i+1){
	write(array[i]+add);
   }
}

void main(){
	real realArray[6]; 
	real a = 2.0;
	int add = 10;

	//给数组赋值
	realArray[0] = 2;
	realArray[1] = a;
	realArray[2] = realArray[0];
	a = 0.9;
	realArray[3] = a;
	realArray[4] = 5 - 2 / (4.0 - 3) * 2.5 + 0.01; 


	function(realArray,add);
}

